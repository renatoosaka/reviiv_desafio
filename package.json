{
  "name": "desafio",
  "type": "module",
  "version": "1.0.0",
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "start": "node dist/src/index.js",
    "predev": "npm run parser",
    "dev": "tsx watch src/index.ts",
    "parser": "tsx src/parser/index.ts",
    "lint": "eslint .",
    "lint:fix": "npm run lint --fix",
    "test": "cross-env NODE_ENV=test npm run parser && vitest",
    "test:coverage": "cross-env NODE_ENV=test npm run parser && vitest run --coverage",
    "prebuild": "npm run parser && rimraf ./dist",
    "build": "npm run parser && tsc && tsc-alias"
  },
  "dependencies": {
    "@hono/node-server": "^1.13.7",
    "@hono/zod-openapi": "^0.18.3",
    "@scalar/hono-api-reference": "^0.5.165",
    "dotenv": "^16.4.7",
    "dotenv-expand": "^12.0.1",
    "hono": "^4.6.16",
    "hono-pino": "^0.7.0",
    "pino": "^9.6.0",
    "pino-pretty": "^13.0.0",
    "rimraf": "^6.0.1",
    "split2": "^4.2.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^3.14.0",
    "@types/node": "^20.11.17",
    "@types/split2": "^4.2.3",
    "@vitest/coverage-istanbul": "^2.1.8",
    "cross-env": "^7.0.3",
    "eslint": "^9.18.0",
    "eslint-plugin-format": "^1.0.1",
    "tsc-alias": "^1.8.10",
    "tsx": "^4.7.1",
    "vitest": "^2.1.8"
  }
}
